<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog id="revInvoiceTypeDialog" contentWidth ="85%"
	multiSelect="false"
		noDataText="{i18n>noDataText}"
		search= "handleSearchRevInv"
		confirm="handleCloseRevInv"
		cancel="_handleValueHelpClose"
		
		items="{
			path : 'revInvoiceModel>/revInvoiceSet'}">
		<ColumnListItem>
			<cells>
				<ObjectIdentifier id="immInvoiceObjectId"
				title="{revInvoiceModel>Fkart}" 
				 />
					<Text text="{revInvoiceModel>Vtext}" />	
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header>
					<Text text="{i18n>RevenueInvoiceType}" />
				</header>
			</Column>
				<Column width="12em">
				<header>
					<Text text="{i18n>text}" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>